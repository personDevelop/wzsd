@model EasyCms.Model.LoginModel
@{
    Layout = null;
    ViewBag.Title = "用户登录-" + CmsSessionExtend.WebSite.Name;
}
@using EasyCms.Model
@using EasyCms
<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/bundles/webcss")
    @Styles.Render("~/bundles/logincss")
    @Scripts.Render("~/bundles/onlyjquery")
    @Scripts.Render("~/bundles/account")
</head>
<body>
    <div>
        <input type="button" value="打开登录" onclick="openlogin()" />
        <input type="button" value="打开正确提示" onclick="openTips()" />
        <input type="button" value="打开错误提示" onclick="openTipsWrong()" />
        

    </div>
</body>
</html>
