                                    <li><a href="javascript:void(0);" value="@DateTime.Now.Year"><b></b>今年订单</a></li>
                                    @{
                                        int year = DateTime.Now.Year;
                                        for (int i = 0; i < 3; i++)
                                        {
                                            year--;
                                            <li><a href="javascript:void(0);" value="@year"><b></b>@(year)年订单</a></li>
                                        }
                                    } 
                                    
                                    <li><a href="javascript:void(0);" value="<@year"><b></b>@(year)年以前订单</a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="order-detail-txt ac">
                            订单详情
                        </div>
                    </th>
                    <th>
                        收货人
                    </th>
                    <th>
                        金额
                    </th>
                    <th>
                        <div class="deal-state-cont " id="orderState" value="-1">
                            <div class="state-txt ">
                                <span>全部状态</span><b></b><span class="blank"></span>
                            </div>
                            <div class="state-list">
                                <ul>
                                    <li value="-1"><a href="javascript:void(0);" class="curr"><b></b>全部状态</a> </li>
                                    <li value="@((int)QryOrderStatus.等待付款)"><a href="javascript:void(0);"><b></b>等待付款</a> </li>
                                    <li value="@((int)QryOrderStatus.等待商家发货)"><a href="javascript:void(0);"><b></b>等待收货</a> </li>
                                    <li value="@((int)QryOrderStatus.完成)"><a href="javascript:void(0);"><b></b>已完成</a> </li>
                                    <li value="@((int)QryOrderStatus.取消订单)"><a href="javascript:void(0);"><b></b>已取消</a> </li>
                                </ul>
                            </div>
                        </div>
                    </th>
                    <th>
                        操作
                    </th>
                </tr>
            </thead>
           
        </table>
    </div>
</div>

<script type="text/javascript">
    function initWrap() {
        var data = getData();
        var ul = $(".order-tb"); 
        ul.find("tbody").empty();
        for (var item in data) {
            ul.append(getItem(data[item]));
        }
    }

    function getItem(obj) {
        var res = [];
        res.push('<tbody><tr class="sep-row"><td colspan="5"></td></tr><tr class="tr-th"><td colspan="5"><span class="gap"></span><span class="dealtime">' + obj.CreateDateStr + '</span>');
        res.push('<span class="number">订单号：<a name="orderIdLinks" target="_blank" href="@Url.Action("Info","order")/' + obj.ID + '">' + obj.ID + '</a></span>');
        res.push('<div class="tr-operate"><span class="order-shop"><span class="shop-txt">联系QQ客服红小七</span> <a target="_blank" href="http://wpa.qq.com/msgrd?v=3&amp;uin=1623591032&amp;site=qq&amp;menu=yes"><img border="0" src="http://wpa.qq.com/pa?p=1:1623591032:7" alt="QQ客服红小七"></a></span>');
        res.push('<a href="javacritp:void(0);" click="deleteOrder(\'' + obj.ID + '\')" class="order-del" title="删除"  style="display: inline;"></a> </div></td></tr>');
        res.push(' <tr class="tr-bd"> <td> ');
        for (var i = 0; i < obj.OrderItems.length; i++) {
            var product = obj.OrderItems[i];
            res.push('<div class="goods-item"><div class="p-img"><a href="@Url.Action("Index", "Product")/' + product.ProductID + '/' + product.ProductSKU + '" target="_blank">');
            res.push('<img src="' + product.ProductThumb + '"  width="60" height="60"> </a> </div><div class="p-msg"><div class="p-name">');
            res.push('<a href="@Url.Action("Index", "Product")/' + product.ProductID + '/' + product.ProductSKU + '" class="a-link" target="_blank">');
            res.push(product.ProductName);
            res.push('</a></div></div></div><div class="goods-number">x' + product.Count + '</div><div class="goods-repair"></div> <div class="clr"></div>');
        }
        res.push('</td> <td rowspan="1"><div class="consignee tooltip"><span class="txt">' + obj.ShipName + '</span><b></b><div class="prompt-01 prompt-02">div class="pc">');
        res.push(' <strong>' + obj.ShipName + '</strong><p>' + obj.ShipAddress);
        res.push('</p> <p>' + obj.ShipTel + '</p></div><div class="p-arrow p-arrow-left"></div></div></div> </td> <td rowspan="1"><div class="amount">');
        res.push(' <span>总额 ¥' + obj.TotalPrice + '</span><br/><span class="ftx-13">' + obj.PayTypeName + '</span> </div></td> <td rowspan="1">');
        res.push('<div class="status"><span class="order-status ftx-03">' + obj.OrderStatusStr + '</span><br/>');
        res.push('<a href="@Url.Action("Info", "order")/' + obj.ID + '" target="_blank">订单详情</a> </div></td><td rowspan="1"><div class="operate"><div></div>');
        res.push('<a href="javascript:void(0);" click="addCard(\'' + product.ProductID + '\',\'' + product.ProductSKU + '\')" class="btn-again" ><b></b>加入购物车</a><br></div> </td></tr></tbody>');
        return res.join('');
    }


    function getData() { 
        $.ajax({
            url: "@Url.Action("GetMyOrder", "Order")",
            type: "get",
            data: { date: $(".ordertime-cont").attr("value"), status: $(".deal-state-cont").attr("value") },
            dataType: "json",
            async: false,
            success: function (reval) {

                reobj = reval;
            },
            error: function (err) {
                throw err.responseText;
                return false;
            }
        });
        return reobj;
    }
    $(function () {

        $(".ordertime-cont .time-txt b").on("click", function () {
            if ($(".ordertime-cont").hasClass("ordertime-hover")) {
                $(".ordertime-cont").removeClass("ordertime-hover");
            } else {
                $(".ordertime-cont").addClass("ordertime-hover");

            } 
        });
        $(".ordertime-cont a").click(function () {

            if (!$(this).hasClass("curr")) {
                $(".ordertime-cont a").removeClass("curr");
                $(this).addClass("curr");
                $(".ordertime-cont .time-txt span").first().text($(this).text());
                $(".ordertime-cont").removeClass("ordertime-hover");
                $(".ordertime-cont").attr("value", $(this).attr("value"));
                initWrap();
            }
        });
        $(".deal-state-cont .state-txt b").on("click", function () {
            if ($(".deal-state-cont").hasClass("deal-state-hover")) {
                $(".deal-state-cont").removeClass("deal-state-hover");
            } else {
                $(".deal-state-cont").addClass("deal-state-hover");

            }
        });
        $(".deal-state-cont li").click(function () {

            if (!$(this).hasClass("curr")) {
                $(".deal-state-cont a").removeClass("curr");
                $(this).addClass("curr");
                $(".deal-state-cont .state-txt span").first().text($(this).text());
                $(".deal-state-cont").removeClass("deal-state-hover");
                $(".deal-state-cont").attr("value", $(this).attr("value"));
                initWrap();
            }
        });


        initWrap();

    });

</script>